---
import MarketingLayout from '../layouts/MarketingLayout.astro';
import '../styles/car-showroom-v3.css';

const title = 'Car Showroom — 3D Studio';
const description =
  'A dedicated, manufacturer-style 3D car showroom: models, materials, colors, camera, and studio backgrounds.';

const base = import.meta.env.BASE_URL ?? '/';
const galleryHref = `${base}gallery/`;
---

<MarketingLayout
  title={title}
  description={description}
  mainClass="p-0"
  bodyClass="bg-black"
  showFooter={false}
  showStickyCTA={false}
  showMobileNav={false}
  showLiveChat={false}
>
  <section class="sr" data-sr-root>
    <div class="sr__grid">
      <header class="sr__top">
        <div class="sr__title">
          <h1>Car Showroom</h1>
          <p>Studio-grade 3D preview • mobile sheet + desktop panel</p>
        </div>
        <div class="sr__actions">
          <a class="sr-btn" href={galleryHref}>3D Gallery</a>
          <button type="button" class="sr-btn sr-btn--icon" data-sr-cmdk-open>
            Search
          </button>
          <button type="button" class="sr-btn sr-btn--icon" data-sr-help-open>
            Help
          </button>
          <button
            type="button"
            class="sr-btn sr-btn--primary sr-btn--icon"
            data-sr-screenshot
          >
            Screenshot
          </button>
          <button type="button" class="sr-btn sr-btn--icon" data-sr-share>
            Copy link
          </button>
          <button
            type="button"
            class="sr-btn sr-btn--icon"
            data-sr-focus-toggle
            aria-pressed="false"
          >
            Focus
          </button>
          <button
            type="button"
            class="sr-btn"
            data-sr-panel-toggle
            aria-expanded="true"
          >
            Panel
          </button>
        </div>
      </header>

      <div class="sr__viewer">
        <canvas class="sr__canvas" data-sr-canvas></canvas>

        <div class="sr__thirds" data-sr-thirds-overlay aria-hidden="true"></div>
        <div class="sr__centerMarker" data-sr-center-overlay aria-hidden="true">
        </div>
        <div class="sr__horizon" data-sr-horizon-overlay aria-hidden="true">
        </div>

        <div class="sr__letterbox" data-sr-letterbox aria-hidden="true"></div>
        <button
          type="button"
          class="sr-btn sr__cinematicExit"
          data-sr-cinematic-exit
          aria-label="Exit cinematic mode"
        >
          Exit cinematic
        </button>

        <div class="sr__quick" data-sr-quick>
          <button
            type="button"
            class="sr__quickBtn"
            data-sr-quick-toggle
            aria-expanded="false"
            aria-label="Open quick controls"
          >
            Quick
          </button>

          <div class="sr__quickMenu" data-sr-quick-menu hidden>
            <div class="sr__quickRow">
              <button type="button" class="sr-chip" data-sr-quick-panel>
                Panel
              </button>
              <button type="button" class="sr-chip" data-sr-cmdk-open>
                Search
              </button>
              <button type="button" class="sr-chip" data-sr-help-open>
                Help
              </button>
              <button
                type="button"
                class="sr-chip"
                data-sr-quick-jump="presets"
              >
                Presets
              </button>
              <button type="button" class="sr-chip" data-sr-quick-jump="tools">
                Tools
              </button>
            </div>

            <div class="sr__quickGrid">
              <label class="sr-field">
                <span>Background</span>
                <select data-sr-quick-bg>
                  <option value="studio">Studio</option>
                  <option value="day">Day</option>
                  <option value="sunset">Sunset</option>
                  <option value="night">Night</option>
                  <option value="grid">Grid</option>
                  <option value="void">Void</option>
                </select>
              </label>

              <label class="sr-field">
                <span>Quality</span>
                <select data-sr-quick-quality>
                  <option value="eco">Eco</option>
                  <option value="balanced">Balanced</option>
                  <option value="ultra">Ultra</option>
                </select>
              </label>

              <label class="sr-field">
                <span>Auto-rotate</span>
                <input
                  type="checkbox"
                  aria-label="Auto-rotate"
                  data-sr-quick-autorotate
                />
              </label>
            </div>

            <div class="sr__quickRow">
              <button type="button" class="sr-chip" data-sr-quick-frame>
                Frame
              </button>
              <button type="button" class="sr-chip" data-sr-quick-photo>
                Photo
              </button>
              <button type="button" class="sr-chip" data-sr-quick-thirds>
                Thirds
              </button>
              <button type="button" class="sr-chip" data-sr-quick-center>
                Center
              </button>
              <button type="button" class="sr-chip" data-sr-quick-horizon>
                Horizon
              </button>
              <button type="button" class="sr-chip" data-sr-quick-cinematic>
                Cine
              </button>
              <button type="button" class="sr-chip" data-sr-quick-interior>
                Interior
              </button>
              <button type="button" class="sr-chip" data-sr-quick-hotspots>
                Hotspots
              </button>
              <button type="button" class="sr-chip" data-sr-quick-decals>
                Decals
              </button>
              <button type="button" class="sr-chip" data-sr-quick-grime>
                Grime
              </button>
              <button type="button" class="sr-chip" data-sr-quick-wet>
                Wet
              </button>
            </div>

            <div class="sr__quickRow">
              <button type="button" class="sr-chip" data-sr-screenshot>
                Screenshot
              </button>
              <button type="button" class="sr-chip" data-sr-share>
                Copy link
              </button>
              <button type="button" class="sr-chip" data-sr-reground>
                Re-ground
              </button>
            </div>
          </div>
        </div>

        <div class="sr__hud">
          <div class="sr__status sr-pill" aria-live="polite">
            <span class="sr-pill__dot"></span>
            <span data-sr-status-loading hidden>Loading…</span>
            <span class="sr-pill__muted" data-sr-status-error hidden></span>
          </div>

          <div class="sr-pill">
            <span class="sr-pill__muted">FPS</span>
            <span data-sr-fps>--</span>
            <span class="sr-pill__muted">RES</span>
            <span data-sr-res>1.00x</span>
            <span class="sr-pill__muted" data-sr-mode>LIVE</span>
            <span class="sr-pill__muted" data-sr-eco hidden>ECO</span>
          </div>
        </div>

        <div class="sr__spec" aria-live="polite" data-sr-specbar>
          <div class="sr__specItem">
            <span>Meshes</span>
            <strong data-sr-spec-meshes>--</strong>
          </div>
          <div class="sr__specItem">
            <span>Materials</span>
            <strong data-sr-spec-mats>--</strong>
          </div>
          <div class="sr__specItem">
            <span>Triangles</span>
            <strong data-sr-spec-tris>--</strong>
          </div>
          <div class="sr__specItem">
            <span>Textures</span>
            <strong data-sr-spec-tex>--</strong>
          </div>
        </div>

        <div class="sr__dock" data-sr-dock>
          <button
            type="button"
            class="sr__dockBtn"
            data-sr-panel-toggle
            aria-label="Toggle panel"
          >
            <svg viewBox="0 0 24 24" aria-hidden="true">
              <path
                d="M5 7h14M5 12h14M5 17h14"
                fill="none"
                stroke="currentColor"
                stroke-width="1.8"
                stroke-linecap="round"></path>
            </svg>
          </button>
          <button
            type="button"
            class="sr__dockBtn"
            data-sr-cmdk-open
            aria-label="Open command palette"
          >
            <svg viewBox="0 0 24 24" aria-hidden="true">
              <path
                d="M4 6h16M4 12h10M4 18h6"
                fill="none"
                stroke="currentColor"
                stroke-width="1.8"
                stroke-linecap="round"></path>
            </svg>
          </button>
          <button
            type="button"
            class="sr__dockBtn"
            data-sr-cinematic-toggle
            aria-label="Toggle cinematic mode"
          >
            <svg viewBox="0 0 24 24" aria-hidden="true">
              <path
                d="M4 7h16v10H4z"
                fill="none"
                stroke="currentColor"
                stroke-width="1.8"></path>
              <path
                d="M7 5v14M17 5v14"
                fill="none"
                stroke="currentColor"
                stroke-width="1.2"></path>
            </svg>
          </button>
          <button
            type="button"
            class="sr__dockBtn"
            data-sr-screenshot
            aria-label="Take screenshot"
          >
            <svg viewBox="0 0 24 24" aria-hidden="true">
              <path
                d="M4 8h4l2-2h4l2 2h4v10H4z"
                fill="none"
                stroke="currentColor"
                stroke-width="1.8"
                stroke-linejoin="round"></path>
              <circle cx="12" cy="13" r="3"></circle>
            </svg>
          </button>
          <button
            type="button"
            class="sr__dockBtn"
            data-sr-focus-toggle
            aria-label="Toggle focus mode"
            aria-pressed="false"
          >
            <svg viewBox="0 0 24 24" aria-hidden="true">
              <path
                d="M7 3H3v4M21 7V3h-4M3 17v4h4M17 21h4v-4"
                fill="none"
                stroke="currentColor"
                stroke-width="1.8"
                stroke-linecap="round"></path>
            </svg>
          </button>
        </div>
      </div>

      <aside class="sr__panel" data-sr-panel>
        <div class="sr__panelHead" data-sr-panel-drag>
          <div>
            <div
              class="sr__panelHandle"
              data-sr-panel-handle
              aria-hidden="true"
            >
            </div>
            <strong>Settings</strong>
          </div>
          <button type="button" class="sr-btn" data-sr-panel-close>Close</button
          >
        </div>

        <div class="sr__panelBody" data-sr-panel-body>
          <input
            type="file"
            accept=".glb,.gltf,.fbx,model/gltf-binary"
            aria-label="Import model file"
            data-sr-model-file
            hidden
          />

          <div
            class="sr__jump sr__tabs"
            aria-label="Jump to section"
            data-sr-jumpbar
          >
            <button type="button" class="sr-chip sr-tab" data-sr-jump="presets">
              <svg viewBox="0 0 24 24" aria-hidden="true">
                <path
                  d="M5 7h14M5 12h10M5 17h6"
                  fill="none"
                  stroke="currentColor"
                  stroke-width="1.8"
                  stroke-linecap="round"></path>
              </svg>
              <span>Presets</span>
            </button>
            <button type="button" class="sr-chip sr-tab" data-sr-jump="model">
              <svg viewBox="0 0 24 24" aria-hidden="true">
                <path
                  d="M4 14l2-4 4-2h6l4 2 2 4-2 4H6z"
                  fill="none"
                  stroke="currentColor"
                  stroke-width="1.8"
                  stroke-linejoin="round"></path>
                <circle cx="8" cy="16" r="1.5"></circle>
                <circle cx="16" cy="16" r="1.5"></circle>
              </svg>
              <span>Body</span>
            </button>
            <button
              type="button"
              class="sr-chip sr-tab"
              data-sr-jump="environment"
            >
              <svg viewBox="0 0 24 24" aria-hidden="true">
                <path
                  d="M4 16l4-4 3 3 5-6 4 7"
                  fill="none"
                  stroke="currentColor"
                  stroke-width="1.8"
                  stroke-linecap="round"
                  stroke-linejoin="round"></path>
              </svg>
              <span>Atmosphere</span>
            </button>
            <button type="button" class="sr-chip sr-tab" data-sr-jump="camera">
              <svg viewBox="0 0 24 24" aria-hidden="true">
                <path
                  d="M4 8h4l2-2h4l2 2h4v10H4z"
                  fill="none"
                  stroke="currentColor"
                  stroke-width="1.8"
                  stroke-linejoin="round"></path>
                <circle cx="12" cy="13" r="3"></circle>
              </svg>
              <span>Camera</span>
            </button>
            <button
              type="button"
              class="sr-chip sr-tab"
              data-sr-jump="performance"
            >
              <svg viewBox="0 0 24 24" aria-hidden="true">
                <path
                  d="M5 19a7 7 0 0 1 14 0"
                  fill="none"
                  stroke="currentColor"
                  stroke-width="1.8"
                  stroke-linecap="round"></path>
                <path
                  d="M12 12l3-3"
                  fill="none"
                  stroke="currentColor"
                  stroke-width="1.8"
                  stroke-linecap="round"></path>
              </svg>
              <span>Performance</span>
            </button>
            <button type="button" class="sr-chip sr-tab" data-sr-jump="tools">
              <svg viewBox="0 0 24 24" aria-hidden="true">
                <path
                  d="M4 6h6v6H4zM14 12h6v6h-6z"
                  fill="none"
                  stroke="currentColor"
                  stroke-width="1.8"></path>
                <path
                  d="M14 6h6M4 18h6"
                  fill="none"
                  stroke="currentColor"
                  stroke-width="1.8"
                  stroke-linecap="round"></path>
              </svg>
              <span>Meta</span>
            </button>
          </div>

          <section
            class="sr-section"
            id="sr-sec-presets"
            data-sr-section="presets"
          >
            <div class="sr-section__head">
              <h2>Presets</h2>
              <button
                type="button"
                class="sr-section__toggle"
                data-sr-section-toggle
                aria-expanded="true"
              >
                Hide
              </button>
            </div>

            <div class="sr-grid">
              <label class="sr-field">
                <span>Name</span>
                <input
                  type="text"
                  aria-label="Preset name"
                  placeholder="My setup"
                  autocomplete="off"
                  data-sr-preset-name
                />
              </label>

              <label class="sr-field">
                <span>Saved</span>
                <select data-sr-preset-select></select>
              </label>

              <div class="sr-row">
                <button
                  type="button"
                  class="sr-btn sr-btn--primary"
                  data-sr-preset-save
                >
                  Save
                </button>
                <button type="button" class="sr-btn" data-sr-preset-load>
                  Load
                </button>
                <button type="button" class="sr-btn" data-sr-preset-delete>
                  Delete
                </button>
              </div>

              <label class="sr-field">
                <span>Export / Import JSON</span>
                <textarea
                  rows="4"
                  placeholder="Paste preset JSON here…"
                  data-sr-preset-io></textarea>
              </label>

              <div class="sr-row">
                <button type="button" class="sr-btn" data-sr-preset-export>
                  Export
                </button>
                <button type="button" class="sr-btn" data-sr-preset-import>
                  Import
                </button>
              </div>
            </div>
          </section>

          <section class="sr-section" id="sr-sec-tools" data-sr-section="tools">
            <div class="sr-section__head">
              <h2>Tools</h2>
              <button
                type="button"
                class="sr-section__toggle"
                data-sr-section-toggle
                aria-expanded="true"
              >
                Hide
              </button>
            </div>

            <div class="sr-row">
              <button type="button" class="sr-btn" data-sr-reground>
                Re-ground
              </button>
              <button type="button" class="sr-btn" data-sr-screenshot-copy>
                Copy screenshot
              </button>
              <button type="button" class="sr-btn" data-sr-share>
                Copy link
              </button>
            </div>

            <div class="sr-grid">
              <label class="sr-field">
                <span>Screenshot scale</span>
                <select data-sr-screenshot-scale>
                  <option value="1">1x (viewport)</option>
                  <option value="2">2x</option>
                  <option value="4">4x</option>
                </select>
              </label>

              <label class="sr-field">
                <span>Cinematic mode</span>
                <input
                  type="checkbox"
                  aria-label="Cinematic mode"
                  data-sr-cinematic
                />
              </label>
            </div>

            <div class="sr-subsection">
              <div class="sr-subsection__head">
                <h3>Layout</h3>
              </div>

              <div class="sr-grid">
                <label class="sr-field">
                  <span>Panel side</span>
                  <select data-sr-panel-side>
                    <option value="right">Right</option>
                    <option value="left">Left</option>
                  </select>
                </label>

                <label class="sr-field">
                  <span>Panel width</span>
                  <input
                    type="range"
                    min="320"
                    max="560"
                    step="10"
                    value="420"
                    aria-label="Panel width"
                    data-sr-panel-width
                  />
                </label>

                <label class="sr-field">
                  <span>Show top bar</span>
                  <input
                    type="checkbox"
                    aria-label="Show top bar"
                    data-sr-layout-top
                  />
                </label>

                <label class="sr-field">
                  <span>Show HUD</span>
                  <input
                    type="checkbox"
                    aria-label="Show HUD"
                    data-sr-layout-hud
                  />
                </label>

                <label class="sr-field">
                  <span>Show spec bar</span>
                  <input
                    type="checkbox"
                    aria-label="Show spec bar"
                    data-sr-layout-specbar
                  />
                </label>

                <label class="sr-field">
                  <span>Show dock</span>
                  <input
                    type="checkbox"
                    aria-label="Show dock"
                    data-sr-layout-dock
                  />
                </label>
              </div>
            </div>

            <div class="sr-grid">
              <label class="sr-field">
                <span>License plate text</span>
                <input
                  type="text"
                  aria-label="License plate text"
                  inputmode="text"
                  maxlength="10"
                  placeholder="DFYTWL"
                  autocomplete="off"
                  data-sr-plate-text
                />
              </label>

              <div class="sr-row">
                <button type="button" class="sr-btn" data-sr-plate-apply>
                  Apply plate
                </button>
                <button type="button" class="sr-btn" data-sr-plate-reset>
                  Reset plate
                </button>
              </div>

              <label class="sr-field">
                <span>Decal / sticker mode</span>
                <input
                  type="checkbox"
                  aria-label="Decal / sticker mode"
                  data-sr-decal-mode
                />
              </label>

              <label class="sr-field">
                <span>Decal text</span>
                <input
                  type="text"
                  aria-label="Decal text"
                  inputmode="text"
                  maxlength="18"
                  placeholder="RACE"
                  autocomplete="off"
                  data-sr-decal-text
                />
              </label>

              <label class="sr-field">
                <span>Decal color</span>
                <input
                  type="color"
                  aria-label="Decal color"
                  value="#ffffff"
                  data-sr-decal-color
                />
              </label>

              <label class="sr-field">
                <span>Decal size</span>
                <input
                  type="range"
                  aria-label="Decal size"
                  min="0.05"
                  max="1.2"
                  step="0.01"
                  value="0.35"
                  data-sr-decal-size
                />
              </label>

              <label class="sr-field">
                <span>Decal rotation</span>
                <input
                  type="range"
                  aria-label="Decal rotation"
                  min="-180"
                  max="180"
                  step="1"
                  value="0"
                  data-sr-decal-rot
                />
              </label>

              <label class="sr-field">
                <span>Decal opacity</span>
                <input
                  type="range"
                  aria-label="Decal opacity"
                  min="0.1"
                  max="1"
                  step="0.01"
                  value="0.92"
                  data-sr-decal-opacity
                />
              </label>

              <div class="sr-row">
                <button type="button" class="sr-btn" data-sr-decal-clear>
                  Clear decals
                </button>
              </div>
            </div>

            <p class="sr-hint">
              Presets save everything you see in the panel (camera, lighting,
              quality, post, etc.).
            </p>

            <p class="sr-hint">Tip: press <kbd>C</kbd> to toggle cinematic.</p>

            <p class="sr-hint">
              Tip: enable decal mode, then click the car to place stickers.
            </p>
          </section>

          <section class="sr-section" id="sr-sec-model" data-sr-section="model">
            <div class="sr-section__head">
              <h2>Model</h2>
              <button
                type="button"
                class="sr-section__toggle"
                data-sr-section-toggle
                aria-expanded="true"
              >
                Hide
              </button>
            </div>
            <div class="sr-grid">
              <label class="sr-field">
                <span>Preset</span>
                <select data-sr-model>
                  <option value="models/porsche-911-gt3rs.glb"
                    >Porsche GT3RS</option
                  >
                  <option
                    value="models/free_porsche_911_carrera_4s_LOD3_low.glb"
                    >Porsche Carrera (Low)</option
                  >
                  <option
                    value="models/free_porsche_911_carrera_4s_advanced_perf.glb"
                    >Porsche Carrera (Perf)</option
                  >
                  <option
                    value="models/free_porsche_911_carrera_4s_advanced_materials.glb"
                    >Porsche Carrera (Materials)</option
                  >
                  <option
                    value="models/free_porsche_911_carrera_4s_LOD1_original_animated.glb"
                    >Porsche Carrera (Animated)</option
                  >
                  <option
                    value="models/free_porsche_911_carrera_4s_LOD0_highpoly_animated.glb"
                    >Porsche Carrera (HiPoly)</option
                  >
                  <option
                    value="models/2003_mitsubishi_eclipse_spyder_gts_shine_street.glb"
                    >Mitsubishi Eclipse Spyder (2003)</option
                  >
                  <option value="models/mclaren_f1_gtr_longtail_ams2.glb"
                    >McLaren F1 GTR Longtail</option
                  >
                  <option
                    value="models/uploads_files_4252681_RED+BULL+2022+F1+CAR.fbx"
                    >F1 Car (FBX - experimental)</option
                  >
                </select>
              </label>

              <label class="sr-field">
                <span>Model URL</span>
                <input
                  type="url"
                  aria-label="Model URL"
                  inputmode="url"
                  placeholder="models/… or https://…"
                  data-sr-model-url
                />
              </label>

              <div class="sr-row">
                <button
                  type="button"
                  class="sr-btn sr-btn--primary"
                  data-sr-model-load>Load</button
                >
                <button type="button" class="sr-btn" data-sr-model-import
                  >Import</button
                >
              </div>

              <p class="sr-hint">
                Tip: use a repo-relative path (models/…) for GitHub Pages.
              </p>

              <div class="sr-grid">
                <label class="sr-field">
                  <span>Scale</span>
                  <input
                    type="range"
                    aria-label="Model scale"
                    min="0.25"
                    max="2"
                    step="0.01"
                    value="1"
                    data-sr-model-scale
                  />
                </label>

                <label class="sr-field">
                  <span>Yaw</span>
                  <input
                    type="range"
                    aria-label="Model yaw"
                    min="-180"
                    max="180"
                    step="1"
                    value="0"
                    data-sr-model-yaw
                  />
                </label>

                <label class="sr-field">
                  <span>Lift</span>
                  <input
                    type="range"
                    aria-label="Model lift"
                    min="-1.5"
                    max="1.5"
                    step="0.01"
                    value="0"
                    data-sr-model-lift
                  />
                </label>
              </div>

              <div class="sr-row">
                <button
                  type="button"
                  class="sr-btn"
                  data-sr-model-transform-reset
                >
                  Reset transform
                </button>
              </div>
            </div>
          </section>

          <section class="sr-section" id="sr-sec-look" data-sr-section="look">
            <div class="sr-section__head">
